template <typename T>
class ListNode {
 public:
 ListNode(ListNode *n, T v): next(n), value(d) {}
  ListNode *get_next() const { return next; }
  T get_value() const { return value; }
  void set_next(ListNode *elem) { next = elem; }
  void set_value(T data) { value = data; }
 private:
  ListNode *next;
  T value;
};
